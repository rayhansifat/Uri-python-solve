A,B,C = list(map(float,input().split()))

area_of_the_rectangled_triangle = 1/2*A*C
area_of_the_radius_circle = 3.14159*C*C
area_of_the_trapezium = (A+B)/2*C
area_of_the_square = B * B
area_of_the_rectangle = A * B

print("TRIANGULO: %0.3f"%area_of_the_rectangled_triangle)
print("CIRCULO: %0.3f"%area_of_the_radius_circle)
print("TRAPEZIO: %0.3f"%area_of_the_trapezium)
print("QUADRADO: %0.3f"%area_of_the_square)
print("RETANGULO: %0.3f"%area_of_the_rectangle)
